---
import { App } from "../components/BuilderEntry";
// @ts-ignore: this package doesn't have types
import { getContent } from "@builder.io/sdk-solid";

const apiKey = "5cd998e3a08d4a9b91194777dc6d52aa";

const builderContent = await getContent({
  model: "page",
  apiKey,
  userAttributes: {
    urlPath: Astro.url.pathname,
  },
});
---

<App client:load builderContent={builderContent} />
